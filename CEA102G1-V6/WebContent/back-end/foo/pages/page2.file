<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<b style="font-size:14px;">共<font color=#bb9d52><%=rowNumber%></font>筆</b>
  <%if (rowsPerPage<rowNumber) {%>
    <%if(pageIndex>=rowsPerPage){%>
         <span style="font-size:14px;">｜ </span><A id="a-color" href="<%=request.getRequestURI()%>?whichPage=1">至第一頁</A>
         <span style="font-size:14px;">｜ </span><A id="a-color" href="<%=request.getRequestURI()%>?whichPage=<%=whichPage-1%>">上一頁</A>
    <%}%>

